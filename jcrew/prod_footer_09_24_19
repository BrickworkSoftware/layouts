jcrew.badgeableProducts = [];
jcrew.productCodesToBadgesMap = {};
jcrew.monetate = {};

jcrew.global.visitsData = getVisitsData();
jcrew.global.categoryVisits = getCategoryVisitsData();

function getVisitsData() {
    var storedVisitsData = window.localStorage.getItem('visits-data');
    var parsedVisitsData = typeof storedVisitsData === 'string' && JSON.parse(storedVisitsData);
    return parsedVisitsData ? JSON.parse(unescape(parsedVisitsData.data)) : null;
}
function getCategoryVisitsData() {
    var storedCategoryVisitsData = window.localStorage.getItem('category-visits');
    var parsedCategoryVisitsData = typeof storedCategoryVisitsData === 'string' && JSON.parse(storedCategoryVisitsData);
    return parsedCategoryVisitsData ? JSON.parse(unescape(parsedCategoryVisitsData.data)) : null;
}
</script>



